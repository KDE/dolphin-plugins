#: makefileactions.cpp:225
#, kde-format
msgid "Make '%1'%2."
msgstr "Make '%1'%2."

#: makefileactions.cpp:254 makefileactions.cpp:262 makefileactions.cpp:264
#, kde-format
msgid "Makefile Actions"
msgstr "«Makefile» ekintzak"

#: makefileactions.cpp:254
#, kde-format
msgid "An error occurred while making target '%1'."
msgstr "Errore bat gertatu da '%1' helburuaren «make» egitean."

#: makefileactions.cpp:262
#, kde-format
msgid "Running process for '%1' successfully stopped."
msgstr "'%1'(e)rako prozesua behar bezala geldiarazi da."

#: makefileactions.cpp:264
#, kde-format
msgid "An error occurred trying to make target '%1'."
msgstr "Errore bat gertatu da '%1' helburuaren «make» egiten saiatzean."

#: makefileactions.cpp:296
#, kde-format
msgid "&Make…"
msgstr "&Make…"

#: makefileactions.cpp:301
#, kde-format
msgid "Only trusted files can be used by the Makefile Actions plugin."
msgstr ""
"«Makefile» ekintzen pluginak, bakarrik, konfiantza duten fitxategiak erabil "
"ditzake."

#: makefileactions.cpp:304
#, kde-format
msgid "Trusted file — uncheck to remove trust"
msgstr "Konfiantza duen fitxategia — kendu marka konfiantza kentzeko"

#: makefileactions.cpp:304
#, kde-format
msgid "Untrusted file — check to trust"
msgstr "Konfiantzarik gabeko fitxategia — markatu konfiatza emateko"

#: makefileactions.cpp:311
#, kde-format
msgid "Dolphin Makefile Plugin"
msgstr "Dolphin «Makefile» plugina"

#: makefileactions.cpp:312
#, kde-format
msgid ""
"<b>Are you sure you can trust this file?</b><br>Trusted files may execute "
"arbitrary code on context-menu invocation."
msgstr ""
"<b>Ziur zaude fitxategi honetan konfiantza izan dezakezula?</"
"b><br>Konfiantza duten fitxategiek, testuinguru-menua deitzen denean, kode "
"arbitrarioa exekutatu dezakete."

#: makefileactions.cpp:326
#, kde-format
msgid "Open a terminal window"
msgstr "Ireki terminal leiho bat"

#: makefileactions.cpp:327
#, kde-format
msgid ""
"Open a new terminal window to see the output of making the chosen target."
msgstr ""
"Ireki terminal leiho berri bat aukeratutako helburuaren «make» egitearen "
"irteera ikusteko."

#: makefileactions.cpp:339
#, kde-format
msgid "Cancel running process (%1)"
msgstr "Ibiltzen ari den prozesua bertan behera utzi (%1)"

#: makefileactions.cpp:340
#, kde-format
msgid "Interrupt the currently running process (%1)."
msgstr "Eten unean ibiltzen ari den prozesua (%1)."
